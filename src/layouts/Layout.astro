---
import DefaultHeader from "../components/DefaultHeader.astro";
interface Props {
  title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="en">
  <head>
    <DefaultHeader {title}>
      <slot name="head" />
    </DefaultHeader>
  </head>
  <body class="bg-background relative bg-noise selection:bg-highlight-off">
    <slot />
  </body>
</html>
