---
// This fixes [locale]/index.astro not being built correctly
import type { GetStaticPaths } from "astro";
import { ui } from "../i18n/ui";

export const getStaticPaths = (() => {
  return Object.keys(ui).map((lang) => ({
    params: {
      locale: lang,
    },
  }));
}) satisfies GetStaticPaths;
---
